@{
    ViewData["Title"] = "Home Page";
}

@model List<Book>

<div class="text-center">
    <h1 class="display-4">Book Store</h1>
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>

    @foreach (Book b in Model)
    {
        <h3>@b.ProjectName</h3>
        <ul>
            <li>Project Type: @b.ProjectType</li>
            <li>Regional Program: @b.ProjectRegionalProgram</li>
            <li>Impact: @b.ProjectImpact individuals served</li>
            <li>Project Phase: @b.ProjectPhase</li>
            <li>Project Status: @b.ProjectFunctionalityStatus</li>
        </ul>
        <br />
    }

</div>

